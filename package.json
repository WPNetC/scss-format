{
    "name": "scss-format",
    "displayName": "scss-format",
    "description": "Format scss files according to linter rules",
    "version": "0.0.1",
    "publisher": "sidsoft",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.lintFormat"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.lintFormat",
                "title": "Lint Format"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
